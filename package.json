    "model",
    "data",
    "database"
  ],
  "author": "Philipp Uhl",
  "license": "UNLICENSED",
  "peerDependencies": {
    "@apparts/types": "^8.0.0"
  },
  "devDependencies": {
    "@apparts/config": "^1.0.6",
    "@apparts/db": "^3.5.1",
    "@apparts/error": "^1.0.3",
    "@apparts/types": "^8.0.0",
    "@tsconfig/node14": "^1.0.3",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.7.2",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.8.2",
    "jest": "^28.1.3",
    "pg": "^8.3.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.7",
    "tsutils": "^3.21.0",
    "typescript": "^4.7.4"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  }
