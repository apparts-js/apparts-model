{
  "name": "@apparts/model",
  "version": "4.7.0",
  "description": "A model for reading and writing to the database. It stores data.",
  "main": "index.js",
  "scripts": {
    "testOne": "jest",
    "test": "jest --watch",
    "testCoverage": "jest --coverage",
    "prepublishOnly": "npm run clean && npm ci  && npm run ci && npm run build",
    "build": "",
    "clean": "rimraf coverage build tmp",
    "ci": "npm ci && npm run ci:prettier && npm run ci:eslint && npm run ci:test",
    "ci:prettier": "CI=true prettier --check .",
    "ci:test": "CI=true jest test",
    "ci:eslint": "eslint . --ext .js,.jsx,.ts,.tsx -c .eslintrc.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:phuhl/apparts-model.git"
  },
  "keywords": [
    "apparts",
    "model",
    "data",
    "database"
  ],
  "author": "Philipp Uhl",
  "license": "UNLICENSED",
  "peerDependencies": {
    "@apparts/types": "^6.1.2 || ^7.0.0"
  },
  "jest": {
    "testTimeout": 10000,
    "watchPathIgnorePatterns": [
      "/\\.#.*",
      "/.*~",
      "/#.*#"
    ],
    "clearMocks": true,
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/tests"
    ]
  },
  "devDependencies": {
    "@apparts/config": "^1.2.3",
    "@apparts/db": "^3.7.0",
    "@apparts/types": "^7.7.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-jest": "^26.1.1",
    "jest": "^27.2.0",
    "pg": "^8.3.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  }
}
